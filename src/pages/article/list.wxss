.page-list{width:750rpx;min-height:100%;overflow:hidden}.page-list .list-tap{padding:0 30rpx;display:-webkit-box;display:flex;-webkit-box-pack:justify;justify-content:space-between;-webkit-box-align:start;align-items:flex-start}.page-list .list-tap .text-cont{font-size:48rpx;font-weight:500}.page-list .list-tap .click-tip{font-size:24rpx;color:#999;font-weight:500}.page-list .list-tap .avatar-box{display:block;height:80rpx;width:80rpx;border-radius:6rpx;overflow:hidden;margin-right:7px;position:relative}.page-list .list-tap .avatar-box .avatar-inner{position:absolute;top:0;right:0;bottom:0;left:0;z-index:9}.page-list .list-tap .avatar-box .open-data{display:block;height:100%;width:100%;position:relative;z-index:0}.page-list .list-item{position:relative;background:#fff;margin:0 30rpx 30rpx;padding:10rpx 30rpx 10rpx;border:1px solid #dedede;border-radius:10rpx;box-shadow:0 6rpx 10rpx #dfdfdf;font-size:32rpx;color:#333;-webkit-transform-origin:50% 100%;transform-origin:50% 100%;-webkit-transform:perspective(600rpx) rotateX(0deg);transform:perspective(600rpx) rotateX(0deg);-webkit-transition:-webkit-transform .8s cubic-bezier(.4, .01, 0, .99);transition:-webkit-transform .8s cubic-bezier(.4, .01, 0, .99);transition:transform .8s cubic-bezier(.4, .01, 0, .99);transition:transform .8s cubic-bezier(.4, .01, 0, .99), -webkit-transform .8s cubic-bezier(.4, .01, 0, .99)}.page-list .list-item.animate{-webkit-transform:perspective(400rpx) rotateX(8deg);transform:perspective(400rpx) rotateX(8deg)}.page-list .list-item .content{padding:25rpx 0;font-weight:700;font-size:36rpx}.foot-info{padding:10rpx 0;font-size:24rpx;color:#999;display:-webkit-box;display:flex;-webkit-box-pack:justify;justify-content:space-between;-webkit-box-align:center;align-items:center}.foot-info .field{display:inline-block}.foot-info .field .text-strong{font-weight:700}.no-data{display:-webkit-box;display:flex;-webkit-box-pack:center;justify-content:center;-webkit-box-align:center;align-items:center;padding:20rpx;color:#999;font-size:28rpx}.page-float-tip .icon-cnode{font-size:50rpx;font-weight:100}.page-float-tip.publish{bottom:240rpx}.page-float-tip.publish .icon-cnode{font-size:42rpx;font-weight:700}